{
  "name": "headingsMap",
  "description": "To show the headings structure of the documents",
  "author": "Rumoroso",
  "manifest_version": 2,
  "version": "3.0.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "<all_urls>",
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_icon": {
      "32": "icons/headingsMap.png"
    },
    "default_title": "headingsMap"
  },
  "web_accessible_resources": [
    "html/*"
  ],
  "options_ui": {
    "page": "options/options.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+O"
      }
    },
    "_execute_sidebar_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+O"
      }
    }
  },
  "sidebar_action": {
    "default_icon": "icons/star.png",
    "default_title" : "Annotator",
    "default_panel": "sidebar/panel.html"
  }
}